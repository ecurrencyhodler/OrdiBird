# Spark SDK Configuration
# Mnemonic seed phrase for the wallet that will mint and receive tokens
SPARK_MNEMONIC=your_mnemonic_seed_phrase_here_with_spaces
SPARK_NETWORK=mainnet
SPARK_RPC_URL=http://localhost:18443
SPARK_RPC_USER=your_rpc_user
SPARK_RPC_PASSWORD=your_rpc_password

# Server Configuration
PORT=3001
NODE_ENV=production

# Rate Limiting
RATE_LIMIT_WINDOW_MS=86400000
RATE_LIMIT_MAX_REQUESTS=1

# Token Configuration
TOKEN_NAME=OrdiBird
TOKEN_TICKER=BIRD
TOKEN_DECIMALS=6
TOKEN_MAX_SUPPLY=0n
TOKEN_IS_FREEZABLE=true
TOKEN_METADATA_URL=https://raw.githubusercontent.com/your-repo/ordibird/main/token-metadata.json
